MODULE test =
BEGIN
    STRUCTURE
        BLK[O,P,S,E;BS,UNIT=4] =
            [BS*UNIT]
            (BLK+O*UNIT)<P,S,E>;
    OWN
        QQQ : BLK [8,BYTE];
        
    PSECT
        NODEFAULT = FARFEL (EXECUTE,OVERLAY),
        GLOBAL = $CODE$,
        CODE = $OWN$;
    OWN
        ALPHA : WORD ALIGN(2) PSECT(FARFEL) VOLATILE,
        BETA  : LONG INITIAL(37) ALIAS,
