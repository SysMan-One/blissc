%NUMBER(-42)
COMPILETIME
    hello = 37, goodbye = -15;
    
%ASCII'-- declarations done--'
hello, goodbye
%ASSIGN(hello,55)
%ASCII'hello is now:' hello
%ASSIGN(hello, goodbye)
%ASCII'hello is now:' hello
%NUMBER('15')
%NUMBER(%X'ff')
%NUMBER(hello)
%ASCII'Declared(hello):' %DECLARED(hello)
%ASCII'Declared(nonexistent):' %DECLARED(nonexistent)
%ASCII'Declared(COMPILETIME):' %DECLARED(COMPILETIME)
MACRO farfel = goodbye%, zikzak = nonexistent%;
%ASCII'Declared(farfel):' %DECLARED(farfel)
%ASCII'Declared(%QUOTE farfel):' %DECLARED(%QUOTE farfel)
%ASCII'Declared(zikzak):' %DECLARED(zikzak)
%ASCII'Declared(%QUOTE zikzak):' %DECLARED(%QUOTE zikzak)
%ASCII'--- NBITS ---'
%NBITS(1) %NBITSU(65535) %NBITS(-1) %NBITSU(1024)
%ASSIGN(hello,5) %ASSIGN(goodbye,8192)
%NBITS(hello,goodbye) %NBITSU(65535,hello,goodbye)

